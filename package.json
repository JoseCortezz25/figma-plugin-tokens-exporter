{
  "name": "figma-code-extractor",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "run-s dev",
    "dev": "run-s watch",
    "dev:ui-only": "vite -c ./vite.config.ui.ts",
    "watch": "run-p watch:*",
    "watch:ui": "npm run build:ui -- --watch --mode=development",
    "watch:plugin": "npm run build:plugin -- --watch --mode=development",
    "clean": "rimraf dist/*",
    "types": "run-s types:*",
    "types:src": "tsc",
    "types:node": "tsc -P tsconfig.node.json",
    "build": "run-s build:*",
    "prebuild": "run-s types clean",
    "build:ui": "vite build -c ./vite.config.ui.ts",
    "build:plugin": "vite build -c ./vite.config.plugin.ts",
    "lint": "pnpm dlx eslint src/**/*.ts"
  },
  "dependencies": {
    "@mapbox/hast-util-to-jsx": "^2.1.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@types/hast": "^3.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/prettier": "^3.0.0",
    "@types/svg-parser": "^2.0.6",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "hast-util-to-html": "^9.0.3",
    "hastscript": "^9.0.0",
    "highlight.js": "^11.10.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.390.0",
    "monorepo-networker": "^1.5.2",
    "prettier-standard": "^16.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "standard": "^17.1.2",
    "svg-parser": "^2.0.4",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@figma/plugin-typings": "^1.96.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.37.1",
    "globals": "^15.10.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.5",
    "sass": "^1.60.0",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.0.11",
    "vite-plugin-generate-file": "^0.1.1",
    "vite-plugin-react-rich-svg": "^1.0.0",
    "vite-plugin-singlefile": "^0.13.5"
  }
}